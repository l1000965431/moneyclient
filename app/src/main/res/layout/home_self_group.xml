                    android:layout_height="15dp"
                    android:id="@+id/home_self_group_iv_exp_new"
                    android:layout_gravity="center"
                    android:src="@mipmap/icon_point"
                    android:tint="#ffff4330"
                    android:layout_marginLeft="-10dp"
