                          android:paddingRight="10dp"
                          android:paddingLeft="40dp" >

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:id="@+id/home_self_group_linearLayout1_portrait"
                    android:layout_gravity="center"
                    android:src="@mipmap/icon_setup"
                    android:scaleType="fitXY"
                    android:tint="#ff40260c" />

                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:id="@+id/home_self_group_linearLayout1_bg"
                    android:layout_gravity="center"
                    android:src="@mipmap/icon_point"
                    android:tint="#ffff4330"
                    android:layout_marginLeft="-10dp"
                    android:layout_marginTop="-20dp"
                    android:visibility="invisible" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="设置"
                    android:id="@+id/home_self_group_linearLayout1_textView"
                    android:textColor="#ff332b38"
                    android:textSize="17dp"
                    android:paddingRight="3dp"
                    android:paddingLeft="7dp"
                    android:layout_gravity="center"
                    android:layout_weight="0.8" />

                <ImageView
                    android:layout_width="17dp"
